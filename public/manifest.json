{
    "manifest_version": 2,
    "name": "NOTE",
    "version": "1.0",
    "description": "NOTE收藏工具",
    "icons": {
        "128": "icon128.png",
        "48": "icon48.png",
        "16": "icon16.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "http://www.mynotemd.com/",
        "tabs"
    ]
}